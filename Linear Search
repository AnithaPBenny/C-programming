#include<stdio.h>  
int main()
{
 int n, i, k, loc=-1, a[10];
 printf("Enter the size of array: ");
 scanf("%d",&n);
 printf("Enter the array elements : ");
 for(i=0; i<n; i++)
    {
		scanf("%d",&a[i]);
	}
 printf("Enter the element to be searched : ");
 scanf("%d",&k);
 for(i=0; i<n; i++)
 {
    if(a[i]==k)
	 {
	   loc=i;
		break;
	 }
 }
 if(loc!=-1)
 {
	printf("The element is found at position : %d",loc+1);
 }
 else
 {
	printf("Element is not present in the array");
 }
 return 0;
}
